<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for development dependencies -->
    <suppress>
        <notes>Suppressing development dependencies</notes>
        <packageUrl regex="true">pkg:npm/.*@.*</packageUrl>
        <cve>CVE-2023-0000</cve>
    </suppress>
    
    <!-- Add specific suppressions for known false positives -->
    <suppress>
        <notes>Known false positive in development environment</notes>
        <packageUrl regex="true">pkg:npm/.*@.*</packageUrl>
        <cve>CVE-2023-0001</cve>
    </suppress>
</suppressions> 